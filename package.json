{
  "name": "vue-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "frontend": "vite",
    "back-end": "json-server src/backend/db.json --port 3001",
    "dev": "concurrently \"npm run frontend\" \"npm run backend\"",
    "dev:frontend": "cd app && npm run dev",
    "dev:backend": "json-server backend/db.json --port 3001",
    "build": "cd app && npm run build",
    "preview": "cd app && npm run preview",
    "backend": "cd app && npm run backend"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@tailwindcss/vite": "^4.1.8",
    "gsap": "^3.12.7",
    "json-server": "^1.0.0-beta.3",
    "vue": "^3.5.13",
    "vue-collapsed": "^1.3.4",
    "vue-router": "^4.5.0",
    "vue3-marquee": "^4.2.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.24",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.42",
    "postcss": "^8.5.4",
    "sass-embedded": "^1.86.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.7.2",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  }
}
